    "\n",
    "otsu_im = []\n",
    "for k in range(len(lst)):\n",
    "    th,bw = cv.threshold(lst[k],0,255,cv.THRESH_BINARY_INV+cv.THRESH_OTSU)\n",
    "    otsu_im.append(bw)\n",
    "    print('Treshold value - ',lst_name[k],'- ' \"{}\".format(th))\n",
    "    \n",
    "fig, ax = plt. subplots(1,3, figsize = (20, 20))\n",
